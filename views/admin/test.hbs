
    <script>
      var weekly = document.getElementById("weekly");
      var monthly = document.getElementById("monthly");
      var yearly = document.getElementById("yearly");

      weekly.onclick = function () {
        alert("weekly")
        $.ajax({
          url: "/admin/date",
          method: "post",
          data: {
            date: "weekly"
          }
        })
      }
      monthly.onclick = function () {
        alert("monthly")
        $.ajax({
          url: "/admin/date",
          method: "post",
          data: {
            date: "monthly"
          }
        })
      }
      yearly.onclick = function () {
        alert("yearly")
        $.ajax({
          url: "/admin/date",
          method: "post",
          data: {
            date: "yearly"
          }
        })
      }
    </script>